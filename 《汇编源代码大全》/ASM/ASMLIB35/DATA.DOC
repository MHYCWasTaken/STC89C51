             mov    es,ax
             assume es:@data
             lea    di,int4
             mov    cx,140        ; search the entire array
             call   maxi4
             shl    ax,1          ; convert dword offset to word offset
             shl    ax,1          ; convert word offset to byte offset
             add    di,ax         ; ES:[DI] points to the maximum value
                                  ; With max/min?4b, the offset of the
                                  ; value is DI + (AX * BX).

